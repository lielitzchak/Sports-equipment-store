@using Sports_equipment_store.Models;

<button>@Html.ActionLink("click to meneger", "ManagerClick", "Shoes")</button>
<button>@Html.ActionLink("show only sale", "OnlySale", "Shoes")</button>


@{ foreach (Shoe shoe in Model)
    {
        <img src="@shoe.LinkToImage" alt="shoes" />
        <h2>type: @shoe.shoeType</h2>
        <h2>company: @shoe.company</h2>
        <h2> price: @shoe.price $</h2>
    }
}
